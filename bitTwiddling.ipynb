   "source": [
    "# Determine if an integer is a power of two through bitwise and (&)\n",
    "def isPowerofTwo(n : int) -> bool:\n",
    "    if n <= 0: return False\n",
    "    else: return n & (n-1) == 0\n",
    "\n",
    "for i in range(9):\n",
    "    print(i, isPowerofTwo(i))"
   ]
